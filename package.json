{
  "name": "@dahlia-labs/mobius-config-registry",
  "version": "0.0.1",
  "description": "Configuration for the Mobius protocol",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mobiusAMM/mobius-config-registry.git"
  },
  "keywords": [
    "celo",
    "blockchain",
    "mobius"
  ],
  "author": "Kyle Scott <kyle@dahlia.finance>",
  "license": "AGPL-3.0",
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@saberhq/eslint-config": "^1.13.20",
    "@types/bn.js": "^5.1.0",
    "@types/eslint": "^8.4.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.36",
    "@types/prettier": "^2.6.3",
    "eslint": "^8.16.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.6.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@dahlia-labs/celo-contrib": "^0.4.5",
    "@dahlia-labs/celo-tokens": "^0.4.5",
    "@dahlia-labs/token-utils": "^0.4.5",
    "@ethersproject/abi": "^5.6.3",
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/bignumber": "^5.6.2",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/constants": "^5.6.1",
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/providers": "^5.6.8",
    "@saberhq/tsconfig": "^1.13.20",
    "@typechain/ethers-v5": "^10.0.0",
    "@yarnpkg/doctor": "^4.0.0-rc.6",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.1",
    "cross-env": "^7.0.3",
    "ethers": "^5.6.8",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "typechain": "^8.0.0",
    "typedoc": "^0.22.16"
  },
  "scripts": {
    "typechain": "typechain --target=ethers-v5 'src/abis/*.json' --out-dir src/generated/",
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -fr dist/",
    "typecheck": "tsc --project tsconfig.cjs.json",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "prepare": "husky install",
    "doctor:packages": "yarn doctor packages/"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json,js,yml,yaml}": "prettier --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/MobiusAmm/mobius-config-registry#readme"
}
